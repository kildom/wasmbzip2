{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "compile",
			"type": "shell",
			"command": "${env:HOME}/Programs/node/bin/node ${workspaceFolder}/scripts/compile.js",
			"problemMatcher": {
				"owner": "cpp",
				"source": "gcc",
				"pattern": {
					"regexp": "^(.*?):([0-9]+)(:([0-9]+))?:(\\s*([a-zA-Z]+):)?\\s*(.*)$",
					"severity": 6,
					"message": 7,
					"file": 1,
					"line": 2,
					"column": 4,
					"code": 6
				}
			},
			"group": "build"
		}
	]
}